<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/bootstrapcss")
</head>
<body>
    <div class ="container-fluid">
       <div class ="row-fluid">
         <div class ="span3">
             <div class =" Well sidebar menu">
                 <ul class =" nav nav-list">
                     <li class ="nav-header"> 1° nav header</li>
                     <li>@Html.ActionLink("Link 1","../")</li>
                     <li>@Html.ActionLink("Link 2","../")</li>
                     <li>@Html.ActionLink("Link 3","../")</li>
                     <li>@Html.ActionLink("Link 4","../")</li>
                     <li class="nav-header"> 2° nav header</li>
                     <li>@Html.ActionLink("Link A","../")</li>
                     <li>@Html.ActionLink("Link B","../")</li>
                     <li>@Html.ActionLink("Link C","../")</li>
                 </ul>
             </div>
        </div>

          <div class ="span9" >
              @RenderSection("featured" , required: false)
              <div class="row-fluid">
                  @RenderBody()
              </div>
      </div>

     </div>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundle/bootstrapjs")
</body>
</html>
